<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.netease.nos.ddbdao.BucketInfoMapper">

    <select id="getBucketName" resultType="String">
      select BucketName from NOS_BucketInfo where ProductId = #{productId} limit 1
    </select>
</mapper>